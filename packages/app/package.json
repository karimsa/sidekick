{
  "private": true,
  "name": "@sidekick/app",
  "version": "0.0.1-3",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "dev": "npm-run-all -p dev-*",
    "dev-web": "next dev -p 9001",
    "dev-build": "node esbuild -w",
    "dev-api": "nodemon -x 'kill-port 9002 && node server.dist.js 2>&1 | tee server.log'",
    "build": "npm-run-all -p build-*",
    "build-web": "next build",
    "build-server": "NODE_ENV=production node esbuild",
    "start": "npm-run-all -p start-*",
    "start-web": "next start",
    "start-api": "nodemon -x '(node server.dist.js 2>&1 | tee ~/.sidekick/server.log) || true'",
    "format": "prettier --write {components,hooks,pages,server,services,utils}",
    "lint": "npm-run-all -p lint-*",
    "lint-eslint": "next lint",
    "lint-tsc": "tsc",
    "lint-prettier": "prettier --check {components,hooks,pages,server,services,utils}",
    "deploy:unstable": "docker build -t ghcr.io/karimsa/sidekick:unstable . && docker push ghcr.io/karimsa/sidekick:unstable",
    "deploy:stable": "docker tag ghcr.io/karimsa/sidekick:unstable ghcr.io/karimsa/sidekick:latest && docker push ghcr.io/karimsa/sidekick:latest"
  },
  "dependencies": {
    "@babel/core": "7.17.0",
    "@babel/plugin-transform-modules-commonjs": "7.16.8",
    "@babel/preset-react": "7.16.7",
    "@babel/preset-typescript": "7.16.7",
    "@esbuild-plugins/node-modules-polyfill": "0.1.4",
    "@monaco-editor/react": "4.3.1",
    "@orthly/context": "0.4.0",
    "@popperjs/core": "2.11.4",
    "@primer/octicons": "16.3.0",
    "@primer/octicons-react": "16.3.0",
    "@tippyjs/react": "4.2.6",
    "axios": "0.25.0",
    "babel-plugin-minify-dead-code-elimination": "0.5.1",
    "body-parser": "1.19.1",
    "classnames": "2.3.1",
    "constate": "3.3.0",
    "cors": "2.8.5",
    "esbuild": "0.14.18",
    "execa": "5",
    "express": "4.17.2",
    "fp-ts": "2.11.8",
    "io-ts": "2.2.16",
    "json-stable-stringify": "1.0.1",
    "kill-port": "1.6.1",
    "lodash": "4.17.21",
    "next": "12.0.10",
    "node-abort-controller": "3.0.1",
    "nodemon": "2.0.15",
    "npm-run-all": "4.1.5",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-error-boundary": "3.1.4",
    "react-hot-toast": "2.2.0",
    "react-popper": "2.2.5",
    "react-query": "3.34.12",
    "react-spring": "9.4.4",
    "resolve": "1.22.0",
    "slugify": "1.6.5",
    "socket.io": "4.4.1",
    "socket.io-client": "4.4.1",
    "terser": "5.10.0",
    "tippy.js": "6.3.7",
    "tmp-promise": "3.0.3",
    "tree-kill": "1.2.2",
    "tslib": "2.3.1",
    "uuid": "8.3.2",
    "zod": "3.14.2"
  },
  "devDependencies": {
    "@types/babel__core": "7.1.18",
    "@types/body-parser": "1.19.2",
    "@types/cors": "2.8.5",
    "@types/debug": "^4.1.7",
    "@types/express": "4.17.13",
    "@types/json-stable-stringify": "1.0.34",
    "@types/lodash": "4.14.178",
    "@types/node": "17.0.14",
    "@types/nodemon": "1.19.1",
    "@types/react": "17.0.39",
    "@types/react-dom": "17.0.2",
    "@types/resolve": "1.20.1",
    "@types/uuid": "8.3.2",
    "acorn": "8.7.0",
    "acorn-walk": "8.2.0",
    "autoprefixer": "10.4.2",
    "postcss": "8.4.6",
    "sass": "1.49.7",
    "tailwindcss": "3.0.18",
    "typescript": "4.5.5",
    "zx": "4.3.0"
  }
}
